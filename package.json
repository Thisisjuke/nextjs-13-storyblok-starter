{
  "name": "@thisisjuke/storyblok-starter",
  "version": "1.0.0",
  "scripts": {
    "start-ssl": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "dev": "npm run start-ssl & next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "storyblok-pull-components": "storyblok pull-components --space=XXXXXX",
    "storyblok-push-components": "storyblok push-components components.XXXXXX.json --space=XXXXXX",
    "storyblok-generate-types": "storyblok-generate-ts source=./components.XXXXXX.json target=./types/storyblok-types",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --output-dir ./.build-storybook"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@splidejs/react-splide": "^0.7.12",
    "@splidejs/splide-extension-auto-scroll": "^0.5.3",
    "@storyblok/react": "^1.3.3",
    "@svgr/webpack": "^6.3.1",
    "next": "^13.0.2",
    "next-seo": "^5.5.0",
    "next-sitemap": "^3.1.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storyblok": "^3.10.5",
    "storyblok-rich-text-react-renderer": "^2.6.1",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.4",
    "@babel/core": "^7.18.13",
    "@netlify/classnames-template-literals": "^1.0.3",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "autoprefixer": "^10.4.8",
    "axios": "^0.26.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.2",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.7.1",
    "storyblok-generate-ts": "^1.6.0",
    "storybook-css-modules": "^1.0.8",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.2",
    "url-loader": "^4.1.1"
  }
}
